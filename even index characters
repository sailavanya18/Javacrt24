import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        String s1=s.nextLine();
        char str[]=s1.toCharArray();
        for(int i=0;i<str.length;i++){
            if(i%2==0){
                System.out.println(i+" "+str[i]);
            }
        }
        
    }
}
