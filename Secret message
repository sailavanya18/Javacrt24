import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        int c=0;
        for(int i=0;i<str.length();i++){
         char ch=str.charAt(i);
            if(ch=='@'||ch=='#'||ch=='$'||ch=='!'||ch=='*'||ch=='&'){
                c++;
            }
        }
        System.out.println(c);
    }
}
