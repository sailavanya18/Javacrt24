import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        for(int i=0;i<n;i++){
            if(i%2==0){
                System.out.print(a[i]+" ");
            }
            }
        for(int i=0;i<n;i++){
            if(i%2==1){
                System.out.print(a[i]+" ");
            }
        }
        }
}
    
