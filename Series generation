import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int r1=7,r2=5;
        int i=1,x;
        x=n;
        n=n/2;
        while(i<=n){
            i++;
            System.out.print(r1+" ");
            r1++;
            System.out.print(r2+" ");
            r2++;
        }
        if(x%2!=0){
            System.out.print(r1);
        }
    }
}
