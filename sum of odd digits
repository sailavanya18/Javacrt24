import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        long n=s.nextLong();
        long d,sum=0;
        while(n>0){
            d=n%10;
            n=n/10;
        if(d%2==1){
            sum=sum+d;
        }
        }
        System.out.println(sum);
    }
}
