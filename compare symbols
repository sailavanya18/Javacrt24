import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        int st=0,h=0;
        for(int i=0;i<str.length();i++){
            char ch= str.charAt(i);
            if(ch=='*'){
                st++;
            }
            else if(ch=='#'){
               h++;
            }
        }
        System.out.println(st-h);
    }
}
