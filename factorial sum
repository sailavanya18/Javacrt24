import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        long sum=0;
        while(n>0){
           int d=n%10;
            int d1=1;
        for(int i=1;i<=d;i++){
           d1=d1*i;
        }
            sum+=d1;
            n=n/10;
        }
    System.out.println(sum);
}
}
