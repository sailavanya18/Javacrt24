import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        long sum=0;
        long a=s.nextLong();
        while(a!=-1){
            sum=sum+a;
            a=s.nextLong();
        }
           
        
        System.out.println(sum);
    }
}
